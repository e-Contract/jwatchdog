<?xml version="1.0" encoding="UTF-8"?>
<jwatchdog-config xmlns="be:e-contract:jwatchdog:1.0"
	sleep="1">
	<datasource name="static">
		<static:static xmlns:static="be:e-contract:jwatchdog:datasource:static:1.0">
			<static:value>1.0</static:value>
			<static:value>2.0</static:value>
			<static:value>3.0</static:value>
		</static:static>
	</datasource>
	<notifier name="mail">
		<mail:mail xmlns:mail="be:e-contract:jwatchdog:notifier:mail:1.0"
			smtpServer="smtp.telenet.be" from="info@e-contract.be" to="info@e-contract.be" />
	</notifier>
	<trigger description="Hello world" datasource="static"
		notifier="mail">
		<condition>
			<above value="2.5" />
		</condition>
	</trigger>
</jwatchdog-config>
